    <h2 id="main-content" class="sr-only-element">Main Content</h2>
    <header>
      <h1>Campus Life</h1>

      
    </header><!-- .entry-header -->

    
<section class="panel-collection" data-modular-content-collection="">

	
  <section class="panel" data-type="wysiwyg" data-modular-content="" data-js="panel" data-index="0" data-width="content">

    
<div class="content">
<p style="text-align: center; font-size: 1.5625em;">A&nbsp;thriving residential campus and community&nbsp;of creative and accomplished people from around the world</p>
</div>


  </section><!-- panel Campus Life Messaging -->



