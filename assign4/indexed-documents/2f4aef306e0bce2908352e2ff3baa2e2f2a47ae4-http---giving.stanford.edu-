var thisTrigger;
var thisPoster;
var thisMovieURL;
function viewTube(triggerID,movieURL,movieWth,movieHt,poster) {
	thisTrigger = triggerID;
	thisPoster = poster;
	thisMovieURL = movieURL;
	var embedcode = '<iframe height="' + movieHt + '" src="' + movieURL + '" frameborder="0" width="' + movieWth + '" allowfullscreen="allowfullscreen"></iframe>';
    document.getElementById(triggerID).innerHTML=embedcode;
  }
  
function clearTube() {
	var resetcode = '<a href="' + thisMovieURL + '" target="_blank" onclick="viewTube(this.parentNode.id,this.href,\'640\',\'360\'); return false"> <img src="' + thisPoster + '" alt="Click to Play" border="0" /></a>';	
	document.getElementById(thisTrigger).innerHTML=resetcode;
}
